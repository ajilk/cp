{
  "languages": {
    "Python": {
      "show_wrap_guides": true,
      "format_on_save": "off"
    }
  },
  "remove_trailing_whitespace_on_save": false,
  "agent": {
    "default_model": {
      "provider": "anthropic",
      "model": "claude-sonnet-4-5-latest"
    },
    "use_modifier_to_send": true,
    "play_sound_when_agent_done": true,
    "model_parameters": []
  },
  "vim_mode": true,
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Zed (Default)"
  },
  "ui_font_size": 16,
  "buffer_font_size": 15,
  "theme": {
    "mode": "light",
    "light": "GitHub Light Classic",
    "dark": "GitHub Dark Classic"
  }
}
